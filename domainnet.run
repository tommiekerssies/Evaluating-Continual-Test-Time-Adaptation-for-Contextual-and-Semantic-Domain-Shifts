  do
      torchrun --nproc_per_node=4 run.py \
      --method ${METHOD} \
      --seed ${SEED} \
      --model best_painting_${SEED}.pth.tar \
      --sources painting \
      -t painting -t real -t clipart -t sketch -t painting \
      --path /dataB3/tommie_kerssies \
      --dataset DomainNet-126 \
      --cycles 1 \
      --batch_size 100 \
      --lr 1e-4
  done
done
